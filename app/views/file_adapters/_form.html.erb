<%= form_with(model: file_adapter, local: true) do |form| %>
  <% if file_adapter.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(file_adapter.errors.count, "error") %> prohibited this file_adapter from being saved:</h2>

      <ul>
      <% file_adapter.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
