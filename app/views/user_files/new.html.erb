<div class="container-fluid">
<%= form_with url: user_files_path, local: true do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :importfile %>
    <%= form.file_field :importfile %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
</div>
